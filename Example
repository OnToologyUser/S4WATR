ex:WaterMeter
    rdf:type owl:Class;
    rdfs:comment "A WaterMeter is a device that measure different aspects of a water infrastructure."^^xsd:string;
    rdfs:subClassOf <https://w3id.org/saref#Sensor>;


ex:ColdWaterMeter
  rdf:type owl:Class;
  rdfs:subClassOf s4watr:WaterMeter;

ex:HotWaterMeter
  rdf:type owl:Class;
  rdfs:subClassOf s4watr:WaterMeter;

